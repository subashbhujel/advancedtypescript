{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/app.ts",
            "cwd": "${workspaceRoot}",
            "sourceMaps": true,
            "outFiles": [ "${workspaceRoot}/app/**/*.js"]
        },
        {
            "type": "node2",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        }
    ]
}
